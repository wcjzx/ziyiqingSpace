<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TabMapper">

    <resultMap id="BaseResultMap" type="com.example.pojo.Tab">
            <id property="tid" column="Tid" jdbcType="INTEGER"/>
            <result property="tabname" column="TabName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        Tid,TabName
    </sql>
</mapper>
